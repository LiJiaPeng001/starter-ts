{
	"name": "@peeeng/utils",
	"type": "module",
	"version": "0.0.1",
	"packageManager": "pnpm@8.1.1",
	"description": "peeeng utils",
	"author": "JiaPeng Li <13673717028m@gmail.com>",
	"license": "MIT",
	"homepage": "https://github.com/lijiapeng001/utils#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lijiapeng001/utils.git"
	},
	"bugs": "https://github.com/lijiapeng001/utils/issues",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"dev": "unbuild --stub",
		"lint": "eslint .",
		"play": "nr -C playground dev",
		"prepublishOnly": "nr build",
		"release": "bumpp && npm publish",
		"start": "esno src/index.ts",
		"test": "vitest",
		"typecheck": "vue-tsc --noEmit"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@antfu/utils": "^0.7.2",
		"@types/node": "^18.15.11",
		"@vitejs/plugin-vue": "^4.1.0",
		"eslint": "^8.37.0",
		"lint-staged": "^13.2.0",
		"pnpm": "^8.1.1",
		"typescript": "^5.0.3",
		"unbuild": "^1.2.0",
		"vite": "^4.2.1",
		"vitest": "^0.29.8",
		"vue": "^3.2.47",
		"vue-tsc": "^1.2.0"
	},
	"lint-staged": {
		"*": "eslint --fix"
	}
}
